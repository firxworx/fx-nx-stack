{
  "name": "fx-nx-stack",
  "description": "nx monorepo for full-stack TypeScript with NextJS (UI) and NestJS (API)",
  "author": "Kevin Firko <kevin@firxworx.com> (https://firxworx.com/)",
  "version": "0.0.0",
  "keywords": [
    "nx",
    "monorepo",
    "NextJS",
    "NextJS"
  ],
  "private": true,
  "scripts": {
    "start": "nx run-many --target=serve --projects=ui,api --parallel",
    "start:ui": "nx serve ui",
    "start:api": "nx serve api",
    "build": "yarn build:all",
    "build:all": "nx run-many --target=build --projects=ui,api --parallel",
    "build:ui": "nx build ui",
    "build:api": "nx build api",
    "build:all:prod": "nx run-many --target=build --projects=ui,api --parallel --prod",
    "test": "nx run-many --target=test --all --parallel",
    "test:ui": "nx test ui",
    "test:api": "nx test api",
    "update": "nx migrate latest"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "core-js": "^3.19.1",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.9",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.2.3",
    "@nrwl/cypress": "13.2.3",
    "@nrwl/eslint-plugin-nx": "13.2.3",
    "@nrwl/jest": "13.2.3",
    "@nrwl/linter": "13.2.3",
    "@nrwl/nest": "13.2.3",
    "@nrwl/next": "13.2.3",
    "@nrwl/node": "13.2.3",
    "@nrwl/react": "13.2.3",
    "@nrwl/storybook": "13.2.3",
    "@nrwl/tao": "13.2.3",
    "@nrwl/web": "13.2.3",
    "@nrwl/workspace": "13.2.3",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "~5.4.0",
    "@typescript-eslint/parser": "~5.4.0",
    "babel-jest": "27.3.1",
    "cypress": "^9.1.0",
    "eslint": "8.3.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "27.3.1",
    "prettier": "^2.5.0",
    "ts-jest": "27.0.7",
    "typescript": "~4.5.2"
  },
  "license": "Apache-2.0"
}

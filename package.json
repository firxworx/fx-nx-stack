{
  "name": "fx-nx-stack",
  "description": "nx monorepo for full-stack TypeScript with NextJS (UI) and NestJS (API)",
  "author": "Kevin Firko <kevin@firxworx.com> (https://firxworx.com/)",
  "version": "0.0.0",
  "keywords": [
    "nx",
    "monorepo",
    "NextJS",
    "NextJS"
  ],
  "private": false,
  "prisma": {
    "seed": "ts-node apps/api/prisma/seed.ts"
  },
  "scripts": {
    "start": "nx run-many --target=serve --projects=ui,api --parallel",
    "start:ui": "nx serve ui",
    "start:api": "nx serve api",
    "generate": "nx generate",
    "prisma:generate": "npx prisma generate --schema apps/api/prisma/schema.prisma",
    "prisma:seed": "npx prisma db seed",
    "prisma:db:push": "npx dotenv -e apps/api/.env -- npx prisma db push --schema apps/api/prisma/schema.prisma",
    "prisma:studio": "npx dotenv -e apps/api/.env -- npx prisma studio --schema apps/api/prisma/schema.prisma",
    "prebuild": "yarn prisma:generate",
    "build": "yarn build:all",
    "build:all": "nx run-many --target=build --projects=ui,api --parallel",
    "build:ui": "nx build ui",
    "build:api": "nx build api",
    "build:ui:prod": "nx build ui --prod",
    "build:api:prod": "nx build api --prod",
    "build:all:prod": "nx run-many --target=build --projects=ui,api --parallel --prod",
    "storybook:lib:react-core": "nx storybook react-core",
    "test:all": "nx run-many --target=test --all --parallel",
    "test:affected": "nx affected:test --all --codeCoverage --skip-nx-cache",
    "test:ui": "nx test ui",
    "test:api": "nx test api",
    "update": "nx migrate latest",
    "docker:postgres:up": "docker compose up -d postgres && exit 0",
    "docker:postgres:down": "docker compose down postgres",
    "docker:postgres:cli": "docker exec -it postgres bash",
    "docker:down": "docker compose down"
  },
  "dependencies": {

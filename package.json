{
  "name": "fx-nx-stack",
  "description": "nx monorepo for full-stack TypeScript with NextJS (UI) and NestJS (API)",
  "author": "Kevin Firko <kevin@firxworx.com> (https://firxworx.com/)",
  "version": "0.0.0",
  "keywords": [
    "nx",
    "monorepo",
    "NextJS",
    "NextJS"
  ],
  "private": true,
  "scripts": {
    "start": "nx run-many --target=serve --projects=ui,api --parallel",
    "start:ui": "nx serve ui",
    "start:api": "nx serve api",
    "build": "nx run-many --target=build --projects=ui,api --parallel",
    "build:ui": "nx build ui",
    "build:api": "nx build api",
    "test": "nx run-many --target=test --all --parallel",
    "test:ui": "nx test ui",
    "test:api": "nx test api",
    "update": "nx migrate latest"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "core-js": "^3.6.5",
    "next": "12.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "13.1.0",
    "@nrwl/cypress": "13.1.0",
    "@nrwl/eslint-plugin-nx": "13.1.0",
    "@nrwl/jest": "13.1.0",
    "@nrwl/linter": "13.1.0",
    "@nrwl/nest": "13.1.0",
    "@nrwl/next": "13.1.0",
    "@nrwl/node": "13.1.0",
    "@nrwl/react": "13.1.0",
    "@nrwl/tao": "13.1.0",
    "@nrwl/web": "13.1.0",
    "@nrwl/workspace": "13.1.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "babel-jest": "27.2.3",
    "cypress": "^8.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.2.3",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  },
  "license": "Apache-2.0"
}
